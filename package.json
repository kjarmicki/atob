{
  "name": "atob",
  "version": "1.0.0",
  "description": "Simple app getting you from point A to B",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm install -g cordova",
    "cordova:create": "cordova create dist/cordova com.github.kjarmicki AtoB",
    "cordova:platform:android": "cd dist/cordova && cordova platform add android@4.0.2 --save",
    "cordova:move": "cp dist/browser/index.html dist/cordova/www/. && cp dist/browser/js/bundle.js dist/cordova/www/js/.",
    "cordova:build": "cd dist/cordova && cordova build",
    "clean": "rm -rf dist",
    "test": "tape -r babel-register 'test/**/*-test.js' | tap-spec",
    "build": "npm run clean && webpack",
    "dev": "google-chrome https://localhost:8080 && webpack-dev-server --host 0.0.0.0 --https",
    "cordova": "npm run build && npm run cordova:create && npm run cordova:platform:android && npm run cordova:move && npm run cordova:build"
  },
  "author": "Krystian Jarmicki",
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "bluebird": "3.4.1",
    "css-loader": "0.23.1",
    "fastclick": "1.0.6",
    "geolib": "2.0.21",
    "html-webpack-plugin": "2.21.0",
    "object-assign": "4.1.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1"
  },
  "devDependencies": {
    "babel-register": "6.9.0",
    "tap-spec": "4.1.1",
    "tape": "4.5.1",
    "webpack-dev-server": "1.14.1"
  }
}
